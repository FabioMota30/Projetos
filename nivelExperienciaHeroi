let heroName = "joseph";
let experience = 600;


if(experience = 1.000){
        console.log("O Heroí de nome " + heroName + " esta no nível de experiência Bronze.");
    
}else if(experience >= 1.001 && experience <= 2.000){
        console.log("O Heroí de nome " + heroName + " esta no nível de experiência Prata.");

}else if(experience >= 2.001 && experience <= 6.000){
        console.log("O Heroí de nome " + heroName + " esta no nível de experiência Ouro.");

}else if(experience >= 6.001 && experience <= 8.000){
        console.log("O Heroí de nome " + heroName + " esta no nível de experiência Platina.");

}else if(experience >= 8.001 && experience <= 9.000){
        console.log("O Heroí de nome " + heroName + " esta no nível de experiência Ascendente.");

}else if(experience >= 9.001 && experience <= 10.000){
        console.log("O Heroí de nome " + heroName + " esta no nível de experiência Imortal.");

}else{
        console.log("O Heroí de nome " + heroName + " esta no nível de experiência Radiante.");
}
